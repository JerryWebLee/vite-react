{
  "name": "operation-and-diagnosis",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && rm -f tsconfig.app.tsbuildinfo tsconfig.node.tsbuildinfo && vite build --base=/",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint \"src/**/*.{css,less}\"",
    "lint:style:fix": "stylelint \"src/**/*.{css,less}\" --fix",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx}'",
    "preview": "vite preview --host",
    "dev:test": "vite --host --mode test",
    "test:build": "vite build --mode test",
    "dev:prod": "vite --host --mode production",
    "prepare-scopes": "node scripts/generateScopes.js",
    "cz": "npm run prepare-scopes && czg",
    "release": "semantic-release --no-ci",
    "prepare": "husky",
    "cz:release": "npm run cz && npm run release"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.0",
    "@ant-design/icons": "^5.5.1",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@tanstack/react-query": "^5.61.3",
    "@tanstack/react-query-devtools": "^5.61.3",
    "ahooks": "^3.8.5",
    "antd": "^5.22.2",
    "axios": "^1.9.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.1",
    "framer-motion": "^11.11.17",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.5",
    "nprogress": "^0.2.0",
    "rc-virtual-list": "^3.18.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0",
    "styled-components": "^6.1.17",
    "tailwind-merge": "^2.5.5",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.15.0",
    "@iconify/json": "^2.2.351",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/node": "^22.9.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "conventional-changelog-angular": "^8.0.0",
    "czg": "^1.11.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "ignore": "^7.0.5",
    "less": "^4.3.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "postcss-less": "^6.0.0",
    "prettier": "3.3.3",
    "semantic-release": "^24.2.5",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-order": "^7.0.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "unplugin-icons": "^22.1.0",
    "vite": "^5.4.10",
    "vite-plugin-compression2": "^1.3.1",
    "vite-plugin-deadfile": "^1.4.0",
    "vite-plugin-stylelint": "^6.0.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/czg"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "**/*.{md,json}": [
      "prettier --write"
    ]
  }
}
